set xlabel "Time (s)"
set ylabel "Current (mA)"

set xrange [0:0.2]

set logscale y
set yrange [5:40]
set ytics (5, 7.5, 10, 15, 20, 25, 30, 35)

set xlabel "Time (s)"
set ylabel "Current (mA)"

set terminal pdf
set out "sleep.pdf"
unset key

plot "partial_fb_refresh" using 1:2 with lines title "Sleep" linecolor rgb "red"
